{
	"name": "obsidian-charted-roots",
	"version": "0.19.7",
	"description": "Family tree visualization for genealogists and worldbuilders. Interactive charts, geographic maps, evidence tracking, GEDCOM support, and fictional calendars.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "npm run build:fonts && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && npm run build:css -- --no-fail-on-lint",
		"build:fonts": "node build-fonts.js",
		"build:css": "node build-css.js",
		"build:css:watch": "node build-css.js --watch",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"lint:css": "stylelint \"styles/**/*.css\"",
		"lint:css:fix": "stylelint \"styles/**/*.css\" --fix",
		"format:css": "prettier --write \"styles/**/*.css\"",
		"deploy": "./deploy.sh",
		"dev:deploy": "./dev-deploy.sh"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"family-tree",
		"genealogy",
		"canvas",
		"d3",
		"charted-roots",
		"canvas-roots"
	],
	"author": "banisterious",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/banisterious/obsidian-charted-roots.git"
	},
	"devDependencies": {
		"@fontsource/roboto-mono": "^5.2.8",
		"@types/d3": "^7.4.3",
		"@types/d3-hierarchy": "^3.1.7",
		"@types/d3-selection": "^3.0.11",
		"@types/leaflet": "^1.9.21",
		"@types/leaflet.markercluster": "^1.5.6",
		"@types/node": "^20.11.5",
		"@types/pdfmake": "^0.2.12",
		"@typescript-eslint/eslint-plugin": "^8.50.0",
		"@typescript-eslint/parser": "^8.50.0",
		"builtin-modules": "^3.3.0",
		"chalk": "^4.1.2",
		"chokidar": "^3.6.0",
		"esbuild": "0.19.11",
		"eslint": "^9.39.2",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"obsidian": "latest",
		"prettier": "^3.6.2",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.5",
		"stylelint-config-standard": "^29.0.0",
		"tslib": "2.6.2",
		"typescript": "5.3.3"
	},
	"dependencies": {
		"d3": "^7.9.0",
		"d3-dag": "^1.1.0",
		"d3-hierarchy": "^3.1.2",
		"d3-selection": "^3.0.0",
		"family-chart": "^0.9.0",
		"jspdf": "^3.0.4",
		"jszip": "^3.10.1",
		"leaflet": "^1.9.4",
		"leaflet-distortableimage": "^0.21.9",
		"leaflet-fullscreen": "^1.0.2",
		"leaflet-minimap": "^3.6.1",
		"leaflet-polylinedecorator": "^1.6.0",
		"leaflet-search": "^4.0.0",
		"leaflet-textpath": "^1.3.0",
		"leaflet.heat": "^0.2.0",
		"leaflet.markercluster": "^1.5.3",
		"pdfmake": "^0.2.20"
	}
}
